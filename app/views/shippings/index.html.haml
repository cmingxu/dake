      %td= shipping.receiver_name
      %td= shipping.receiver_tel
      %td= shipping.receiver_address
      %td= shipping.fee
      %td= shipping.huowu_name
      %td= shipping.huowu_danwei
      %td= shipping.huowu_amount
      %td= shipping.huowu_beizhu
      %td= shipping.status
      %td= shipping.is_paid
      %td= shipping.is_daishouhuokuan
      %td= shipping.huowu_cost
      %td= shipping.huowu_baozhi
      %td= shipping.baozhi_needed
      %td= shipping.daishouhuokuan_fee
      %td= link_to 'Show', shipping
      %td= link_to 'Edit', edit_shipping_path(shipping)
